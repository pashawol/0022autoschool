mixin s-contact()
	// start s-contact
	+b.FOOTER.s-contact.section(data-bg="img/@2x/pattern.jpg")#s-contact&attributes(attributes)
		.container
			if block
				block
			else
				+b.section-title.text-center
					h2 контактная информация
					+e.text Проводим обучение в&nbsp;автозаводском, центральном и&nbsp;шлюзовом районах
			+contact-line()
			+e.text Не&nbsp;откладывайте! Пока вы&nbsp;думаете, другие уже обучаются в&nbsp;нашей автошколе: участвуют в&nbsp;конкурсах, пользуются вебинарной платформой бесплатно и&nbsp;получают водительские права в&nbsp;кротчайшие сроки.
			+e.car-wrap
				+pic("footer-car.png")
				+pic("footer-car-active.png")(class="picture-active")
			+e.foot.row
				.col-lg-auto
					a.link-modal(href="#modal-policy" ) политика конфиденциальности
				.col-lg-auto
					| Разработка сайта – 
					| 
					a.link(href="https://vk.com/ishmetovm" target="_blank") Ishmetov
			//- .row
		
	// end s-contact
	+b.mobile-tel.btn-js(href="tel:+7(927)268-36-77") +7 (927) 268-36-77
	+b.alert-footer.link-modal(href="#modal-call" data-title="записаться в 1 клик" 
		data-btn="записаться в 1 клик" data-order="записаться в 1 клик c красной полосы")
		marquee( loop="-1")
			- for (var x = 0; x < 10; x++)
				| новые тарифы: стандартный - 9000 руб, экономный - 14000 руб, комфортный - 16000 руб, эффективный - 19000 руб, персональный - 24000 руб 
		+e.DIV.btn-wrap
			+e.DIV.btn.btn-js записаться в 1 клик
